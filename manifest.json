{
    "name": "MEDIUM-HISTORY",
    "short_name": "MEDIUM-HISTORY",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://medium.com/*"
            ],
            "all_frames": true,
            "run_at": "document_end",
            "js": [
                "content.js"
            ]
        }
    ]
}